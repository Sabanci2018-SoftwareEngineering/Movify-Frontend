{
    "extends": "rallycoding",
    "rules": {
        "react/jsx-boolean-value": "off",
        "brace-style": "off",
        "max-len": "off",
        "global-require": "off",
        "quote-props": "off",
        "no-else-return": "off",
        "prefer-template": "off",
        "arrow-body-style": "off",
        "no-unused-expressions": "off",
        "no-useless-escape": "off",
        "object-shorthand": "off"
    }
}